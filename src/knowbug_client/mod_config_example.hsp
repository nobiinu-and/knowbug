// mod_config の使用例

#include "hsp3utf.as"
#include "mod_config.hsp"

	config_load "mod_config_example.conf", keys, values
	assert stat == 4

	assert keys(0) == "hello"
	assert values(0) == "world"

	assert keys(1) == "greet"
	assert values(1) == "你好😃"

	assert keys(2) == "tightkey"
	assert values(2) == "tightvalue"

	assert keys(3) == "indented_key"
	assert values(3) == "spaced  value"

	logmes "finish"
